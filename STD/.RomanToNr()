def RomanToInt(s):
    Roman_Number_Dict_With_Values = {"i": 1, "v": 5, "x": 10, "l": 50, "c": 100, "d": 500, "m": 1000}
    Number_Minus_One = 0
    End_Number = 0
    for Roman_Number in s.lower():
        Number = Roman_Number_Dict_With_Values[Roman_Number]
        if Number > Number_Minus_One:
            End_Number += Number - Number_Minus_One * 2
        else:
            End_Number += Number
        Number_Minus_One = Number
    return End_Number
